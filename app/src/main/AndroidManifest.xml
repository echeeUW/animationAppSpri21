<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uw.echee.myanimationapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyAnimationAppSpr21">
        <activity android:name=".viewpager.SwipePagesActivity"></activity>
        <activity android:name=".activitytransition.EndTransitionActivity" />
        <activity android:name=".activitytransition.StartTransitionActivity" />
        <activity android:name=".autoanimate.AutoAnimateActivity" />
        <activity android:name=".basicanimation.BasicAnimationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
